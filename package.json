{
  "name": "glue",
  "version": "1.0.0",
  "description": "AI-powered website design system analyzer and component generator",
  "main": "backend/src/server.js",
  "scripts": {
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:frontend": "cd frontend && npx react-scripts start",
    "start:backend": "npx ts-node-dev --respawn backend/src/server.ts",
    "build": "cd frontend && npx react-scripts build",
    "build:backend": "npx tsc --project backend/tsconfig.json",
    "test": "cd frontend && npx react-scripts test",
    "dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\""
  },
  "keywords": [
    "ai",
    "design-system",
    "website-analyzer",
    "component-generator"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.1",
    "@reduxjs/toolkit": "^2.2.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^20.19.0",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "axios": "^1.9.0",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "puppeteer": "^22.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "postcss": "^8.5.4",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0"
  }
}
